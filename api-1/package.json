{
    "name": "api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "reset": "docker rm -f $(docker ps -aq); docker rmi -f $(docker images -aq); docker volume rm $(docker volume ls -q); docker builder prune -af; rm -rf ./prisma/migrations",
        "dev": "clear; eslint \"{src,apps,libs,test}/**/*.ts\" --fix; docker compose up -d db && npx prisma db push --force-reset && npx prisma db seed && nest start --debug --watch",
        "prod": "node dist/src/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix; jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.806.0",
        "@aws-sdk/s3-request-presigner": "^3.806.0",
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/common": "^10.4.17",
        "@nestjs/config": "^3.2.2",
        "@nestjs/core": "^10.4.17",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.4.17",
        "@nestjs/platform-socket.io": "^10.4.17",
        "@nestjs/schedule": "^4.0.2",
        "@nestjs/swagger": "^7.3.1",
        "@nestjs/websockets": "^10.4.17",
        "@prisma/adapter-pg": "^5.11.0",
        "@prisma/client": "^6.19.2",
        "@sendgrid/mail": "^8.1.5",
        "@types/multer": "^1.4.11",
        "@types/unzipper": "^0.10.11",
        "adm-zip": "^0.5.16",
        "ajv": "^6.12.6",
        "archiver": "^7.0.1",
        "argon2": "^0.40.1",
        "axios": "^1.10.0",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "crypto": "^1.0.1",
        "csv-parser": "^3.2.0",
        "fs-extra": "^11.3.0",
        "handlebars": "^4.7.8",
        "multer": "^2.0.0",
        "nodemailer": "^7.0.3",
        "p-queue": "^6.6.2",
        "papaparse": "^5.5.3",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "pdfkit": "^0.17.1",
        "pdfkit-table": "^0.1.99",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "stream-buffers": "^3.0.3",
        "string-similarity": "^4.0.4",
        "ts-morph": "^26.0.0",
        "unzipper": "^0.12.3",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.4.17",
        "@types/adm-zip": "^0.5.7",
        "@types/axios": "^0.9.36",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.17",
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/nodemailer": "^6.4.17",
        "@types/p-queue": "^3.1.0",
        "@types/papaparse": "^5.3.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/pdfkit": "^0.14.0",
        "@types/string-similarity": "^4.0.2",
        "@types/supertest": "^6.0.0",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "dotenv-cli": "^8.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "prisma": "^6.19.2",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/$1"
        }
    }
}
